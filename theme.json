{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [
		         {
					"colors": [ "#1d1f35", "#6cace4" ],
					"slug": "dark-blue-and-light-blue",
					"name": "Dark blue and light blue"
				},
				{
					"colors": [ "#1d1f35", "#c7c0af" ],
					"slug": "dark-blue-and-beige",
					"name": "Dark blue and beige"
				}
			],
			"gradients": [
				{
					"slug": "primary-to-secondary",
					"gradient": "linear-gradient(160deg, var(--wp--preset--color--primary), var(--wp--preset--color--secondary))",
					"name": "Primary to Secondary"
				}
			],
			"palette": [
				{
					"color": "#009ffe",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#ff5733",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#ffc300",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#d780d0",
					"name": "Color 100",
					"slug": "color-100"
				},
				{
					"color": "#fefecc",
					"name": "Color 200",
					"slug": "color-200"
				},
				{
					"color": "#91d780",
					"name": "Color 300",
					"slug": "color-300"
				},
				{
					"color": "#17171a",
					"name": "Main",
					"slug": "main"
				},
				{
					"color": "#0a0a0a",
					"name": "Main Alt",
					"slug": "main-alt"
				},
				{
					"color": "#fefefe",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#343434",
					"name": "Grey 100",
					"slug": "grey-100"
				},
				{
					"color": "#1e1e23",
					"name": "Grey 200",
					"slug": "grey-200"
				},
				{
					"color": "#878787",
					"name": "Grey 300",
					"slug": "grey-300"
				},
				{
					"color": "#bebebe",
					"name": "Grey 400",
					"slug": "grey-400"
				},
				{
					"color": "#dcdcdc",
					"name": "Grey 500",
					"slug": "grey-500"
				}
			],
			"link": true
		},
		"custom": {
			"fontWeight": {
				"thin": 100,
				"extra-light": 200,
				"light": 300,
				"regular": 400,
				"medium": 500,
				"semi-bold": 600,
				"bold": 700,
				"extra-bold": 800,
				"black": 900
			},
			"lineHeight": {
				"none": 1,
				"tight": 1.1,
				"snug": 1.2,
				"normal": 1.5,
				"relaxed": 1.625,
				"loose": 2,
				"body": "calc(1px + 2ex + 7px)"
			}
		},
		"layout": {
			"contentSize": "960px",
			"wideSize": "1260px"
		},
		"spacing": {
			"spacingSizes": [
				{
					"name": "Small",
					"size": "clamp(.5rem, 2.5vw, 1rem)",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "clamp(1.5rem, 4vw, 2rem)",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "clamp(2rem, 5vw, 3rem)",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "clamp(3rem, 7vw, 5rem)",
					"slug": "x-large"
				},
				{
					"name": "2xl",
					"size": "clamp(4rem, 9vw, 7rem)",
					"slug": "xx-large"
				},
				{
					"name": "3xl",
					"size": "clamp(5rem, 12vw, 9rem)",
					"slug": "xxx-large"
				},
				{
					"name": "4xl",
					"size": "clamp(6rem, 14vw, 13rem)",
					"slug": "xxxx-large"
				}
			]
		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "'Montserrat', Arial, sans-serif",
					"name": "Montserrat",
					"slug": "primary",
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Montserrat",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "100 900",
							"src": [
								"file:.\/fonts\/Montserrat-VariableFont_wght.woff2"
							]
						}
					]
				},
				{
					"fontFamily": "monospace",
					"name": "Monospace",
					"slug": "monospace"
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"min": "0.8125rem",
						"max": "0.875rem"
					},
					"size": "0.875rem",
					"slug": "xx-small",
					"name": "2-XS"
				},
				{
					"fluid": {
						"min": "0.875rem",
						"max": "0.9375rem"
					},
					"size": "0.9375rem",
					"slug": "x-small",
					"name": "1-XS"
				},
				{
					"fluid": {
						"min": "0.9375rem",
						"max": "1rem"
					},
					"size": "1rem",
					"slug": "small",
					"name": "Small"
				},
				{
					"fluid": {
						"min": "1rem",
						"max": "1.125rem"
					},
					"size": "1.125rem",
					"slug": "base",
					"name": "Base"
				},
				{
					"fluid": {
						"min": "1.125rem",
						"max": "1.25rem"
					},
					"size": "1.25rem",
					"slug": "medium",
					"name": "Medium"
				},
				{
					"fluid": {
						"min": "1.25rem",
						"max": "1.375rem"
					},
					"size": "1.375rem",
					"slug": "large",
					"name": "Large"
				},
				{
					"fluid": {
						"min": "1.375rem",
						"max": "1.5rem"
					},
					"size": "1.5rem",
					"slug": "x-large",
					"name": "1-XL"
				},
				{
					"fluid": {
						"min": "1.5rem",
						"max": "1.625rem"
					},
					"size": "1.625rem",
					"slug": "xx-large",
					"name": "2-XL"
				},
				{
					"fluid": {
						"min": "1.625rem",
						"max": "2rem"
					},
					"size": "2rem",
					"slug": "huge",
					"name": "Huge"
				},
				{
					"fluid": {
						"min": "2rem",
						"max": "3.25rem"
					},
					"size": "3.25rem",
					"slug": "gigantic",
					"name": "Gigantic"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			
		},
		"color": {
			"background": "var(--wp--preset--color--main)",
			"text": "var(--wp--preset--color--base)"
		},
		"elements": {
			"heading": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--base)",
					"fontWeight": "var(--wp--custom--font-weight--regular)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--gigantic)",
					"fontWeight": "var(--wp--custom--font-weight--thin)"
				}
			},
			"h2": {
				"color": {
					"text": "var(--wp--preset--color--secondary)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)"
				}
			},
			"h3": {
				"color": {
					"text": "var(--wp--preset--color--secondary)"
				}
			},
			"h4": {
				"color": {
					"text": "var(--wp--preset--color--secondary)"
				}
			},
			"h5": {
				"color": {
					"text": "var(--wp--preset--color--secondary)"
				}
			},
			"h6": {
				"color": {
					"text": "var(--wp--preset--color--secondary)"
				}
			}
		},
		"spacing": {

		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--primary)",
			"fontSize": "var(--wp--preset--font-size--base)",
			"fontWeight": "var(--wp--custom--font-weight--regular)",
			"lineHeight": "var(--wp--custom--line-height--loose)",
			"letterSpacing": "normal",
			"textTransform": "none",
			"fontStyle": "normal"
		}
	}
}