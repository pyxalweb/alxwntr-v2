{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [
		         {
					"colors": [ "#1d1f35", "#6cace4" ],
					"slug": "dark-blue-and-light-blue",
					"name": "Dark blue and light blue"
				},
				{
					"colors": [ "#1d1f35", "#c7c0af" ],
					"slug": "dark-blue-and-beige",
					"name": "Dark blue and beige"
				}
			],
			"gradients": [
				{
					"slug": "primary-to-secondary",
					"gradient": "linear-gradient(160deg, var(--wp--preset--color--primary), var(--wp--preset--color--secondary))",
					"name": "Primary to Secondary"
				}
			],
			"palette": [
				{
					"color": "--c--primary",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "--c--secondary",
					"name": "Secondary",
					"slug": "secondary"
				}
			],
			"link": true
		},
		"custom": {
			"fontWeight": {
				"thin": 100,
				"extra-light": 200,
				"light": 300,
				"regular": 400,
				"medium": 500,
				"semi-bold": 600,
				"bold": 700,
				"extra-bold": 800,
				"black": 900
			},
			"lineHeight": {
				"none": 1,
				"tight": 1.1,
				"snug": 1.2,
				"normal": 1.5,
				"relaxed": 1.625,
				"loose": 2,
				"body": "calc(1px + 2ex + 7px)"
			}
		},
		"layout": {
			"contentSize": "960px",
			"wideSize": "1260px"
		},
		"spacing": {

		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "'Montserrat', Arial, sans-serif",
					"name": "Montserrat",
					"slug": "primary",
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Montserrat",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400 700",
							"src": [
								"file:.\/fonts\/Montserrat-VariableFont_wght.woff2"
							]
						}
					]
				},
				{
					"fontFamily": "monospace",
					"name": "Monospace",
					"slug": "monospace"
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"min": "0.8125rem",
						"max": "0.875rem"
					},
					"size": "0.875rem",
					"slug": "xx-small",
					"name": "2-XS"
				},
				{
					"fluid": {
						"min": "0.875rem",
						"max": "0.9375rem"
					},
					"size": "0.9375rem",
					"slug": "x-small",
					"name": "1-XS"
				},
				{
					"fluid": {
						"min": "0.9375rem",
						"max": "1rem"
					},
					"size": "1rem",
					"slug": "small",
					"name": "Small"
				},
				{
					"fluid": {
						"min": "1rem",
						"max": "1.125rem"
					},
					"size": "1.125rem",
					"slug": "base",
					"name": "Base"
				},
				{
					"fluid": {
						"min": "1.125rem",
						"max": "1.25rem"
					},
					"size": "1.25rem",
					"slug": "medium",
					"name": "Medium"
				},
				{
					"fluid": {
						"min": "1.25rem",
						"max": "1.375rem"
					},
					"size": "1.375rem",
					"slug": "large",
					"name": "Large"
				},
				{
					"fluid": {
						"min": "1.375rem",
						"max": "1.5rem"
					},
					"size": "1.5rem",
					"slug": "x-large",
					"name": "1-XL"
				},
				{
					"fluid": {
						"min": "1.5rem",
						"max": "1.625rem"
					},
					"size": "1.625rem",
					"slug": "xx-large",
					"name": "2-XL"
				},
				{
					"fluid": {
						"min": "1.625rem",
						"max": "2rem"
					},
					"size": "2rem",
					"slug": "huge",
					"name": "Huge"
				},
				{
					"fluid": {
						"min": "2rem",
						"max": "3.25rem"
					},
					"size": "3.25rem",
					"slug": "gigantic",
					"name": "Gigantic"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			
		},
		"color": {
			"background": "#000",
			"text": "#fff"
		},
		"elements": {

		},
		"spacing": {

		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--primary)",
			"fontSize": "var(--wp--preset--font-size--base)",
			"fontWeight": "var(--wp--custom--font-weight--regular)",
			"lineHeight": "var(--wp--custom--line-height--body)"
		}
	}
}